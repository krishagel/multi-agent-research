# SECURITY NOTE: Never commit this file with real credentials!
# Copy this file to .env and add your actual API keys

# API Keys
TAVILY_API_KEY=your_tavily_api_key_here

# AWS Bedrock Configuration
# For production, prefer using IAM roles instead of access keys
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# Model Configuration
LEAD_RESEARCHER_MODEL=anthropic.claude-3-sonnet-20240229-v1:0
SUB_RESEARCHER_MODEL=anthropic.claude-3-haiku-20240307-v1:0
CITATION_MODEL=anthropic.claude-3-haiku-20240307-v1:0

# Search Configuration
TAVILY_SEARCH_DEPTH=basic  # basic or advanced
MAX_SEARCH_RESULTS=5

# Cost Tracking
MONTHLY_BUDGET_USD=50
ALERT_AT_PERCENT=80